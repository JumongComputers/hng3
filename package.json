{
  "name": "hng3ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "nodemon": "nodemon --exec ts-node src/server.ts",
   "migrate": "npx sequelize-cli db:migrate --config src/config/db.config.cjs --migrations-path dist/migrations",
    "rollback": "npx sequelize-cli db:migrate:undo --config dist/config/db.config.cjs --migrations-path dist/migrations",
    "db:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize db:seed:all",
    "seed:undo": "npx sequelize db:seed:undo:all",
    "mg": "npx sequelize-cli migration:generate --name"
  },
  "dependencies": {
    "@types/sequelize": "^4.28.20",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.4.4",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
